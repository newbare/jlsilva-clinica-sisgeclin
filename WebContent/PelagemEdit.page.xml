<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/PelagemList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{pelagemHome.wire}"/>

   <param name="pelagemFrom"/>
   <param name="pelagemCodpelagem" value="#{pelagemHome.pelagemCodpelagem}"/>


   <navigation from-action="#{pelagemHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Pelagem.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{pelagemHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Pelagem.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{pelagemHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PelagemList.xhtml"/>
      </rule>
   </navigation>

</page>
