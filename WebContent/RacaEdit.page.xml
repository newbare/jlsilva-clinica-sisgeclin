<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/RacaList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{racaHome.wire}"/>

   <param name="racaFrom"/>
   <param name="racaCodraca" value="#{racaHome.racaCodraca}"/>
   <param name="especiesFrom"/>
   <param name="especiesCodespecie" value="#{especiesHome.especiesCodespecie}"/>



   <navigation from-action="#{racaHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Raca.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{racaHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Raca.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{racaHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/RacaList.xhtml"/>
      </rule>
   </navigation>

</page>
